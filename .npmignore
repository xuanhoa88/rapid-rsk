# NPM ignore patterns
# Files to exclude when publishing to npm registry

# Package manager files
package-lock.json
yarn.lock
pnpm-lock.yaml
.npmrc
.yarnrc
.pnpm-workspace.yaml

# Development dependencies
node_modules/

# Build artifacts
coverage/
.nyc_output/
*.log

# IDE and editor files
.idea/
.vscode/
*.swp
*.swo
*~
.DS_Store

# Git files
.git/
.gitignore
.gitattributes
.github/

# CI/CD
.travis.yml
.gitlab-ci.yml
circle.yml
appveyor.yml

# Docker
Dockerfile
.dockerignore
docker-compose.yml
docker-compose.*.yml

# Testing
__tests__/
__mocks__/
*.test.js
*.spec.js
*.test.jsx
*.spec.jsx

# Documentation
*.md
!README.md
!LICENSE.md
!CHANGELOG.md

# Environment files
.env
.env.*

# Misc
.cache/
tmp/
temp/
